{
  "name": "retail",
  "version": "1.0.0",
  "private": true,
  "description": "Refined starter template for building full-stack web applications using React Router 7, TypeScript, Tailwind CSS, Prettier and ESLint.",
  "homepage": "https://retail.nikolailehbr.ink/",
  "bugs": {
    "url": "https://github.com/nikolailehbrink/retail/issues",
    "email": "mail@nikolailehbr.ink"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nikolailehbrink/retail.git"
  },
  "author": "Nikolai Lehbrink <mail@nikolailehbr.ink> (https://www.nikolailehbr.ink/)",
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "check:unused": "knip",
    "clean": "rm -rf node_modules",
    "dev": "react-router dev",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:inspect": "eslint --inspect-config",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc"
  },
  "dependencies": {
    "@fontsource-variable/inter": "^5.2.6",
    "@react-router/node": "^7.6.2",
    "@react-router/serve": "^7.6.2",
    "isbot": "^5.1.28",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.6.2"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.0",
    "@eslint/js": "^9.29.0",
    "@react-router/dev": "^7.6.2",
    "@tailwindcss/vite": "^4.1.10",
    "@types/node": "^22",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "eslint": "^9.29.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.2.0",
    "knip": "^5.61.3",
    "prettier": "^3.5.3",
    "prettier-plugin-classnames": "^0.7.9",
    "prettier-plugin-merge": "^0.7.4",
    "prettier-plugin-tailwindcss": "^0.6.12",
    "tailwindcss": "^4.1.10",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "engines": {
    "node": "22.x"
  }
}
