{
  "name": "retail",
  "version": "1.0.0",
  "private": true,
  "description": "Refined starter template for building full-stack web applications using React Router 7, TypeScript, Tailwind CSS, Prettier and ESLint.",
  "homepage": "https://retail.nikolailehbr.ink/",
  "bugs": {
    "url": "https://github.com/nikolailehbrink/retail/issues",
    "email": "mail@nikolailehbr.ink"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nikolailehbrink/retail.git"
  },
  "author": "Nikolai Lehbrink <mail@nikolailehbr.ink> (https://www.nikolailehbr.ink/)",
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "check:unused": "knip",
    "clean": "rm -rf node_modules",
    "dev": "react-router dev",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:inspect": "eslint --inspect-config",
    "preview": "vite preview",
    "typecheck": "react-router typegen && tsc"
  },
  "dependencies": {
    "@fontsource-variable/inter": "5.2.8",
    "@react-router/node": "7.11.0",
    "@react-router/serve": "7.11.0",
    "isbot": "5.1.32",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-router": "7.11.0"
  },
  "devDependencies": {
    "@eslint/compat": "2.0.0",
    "@eslint/js": "9.39.2",
    "@react-router/dev": "7.11.0",
    "@tailwindcss/vite": "4.1.18",
    "@types/node": "22",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "eslint": "9.39.2",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "globals": "16.5.0",
    "knip": "5.78.0",
    "prettier": "3.7.4",
    "prettier-plugin-classnames": "0.8.6",
    "prettier-plugin-merge": "0.8.0",
    "prettier-plugin-tailwindcss": "0.7.2",
    "tailwindcss": "4.1.18",
    "typescript": "5.9.3",
    "typescript-eslint": "8.51.0",
    "vite": "7.3.0",
    "vite-tsconfig-paths": "6.0.3"
  },
  "engines": {
    "node": "22.x"
  }
}
